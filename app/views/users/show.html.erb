<h1>Perfil</h1>
	
<table class="perfil">
	<tr>
		<td class="main">
			<h2>
				<%=gravatar_for @user, :size => 50%>
				<%= @user.nombre %>
			</h2>			
		</td>
		<td class="sidebar round">
			<strong>Usuario Creado en:</strong> <%=@user.created_at%><br />			
		</td>
	</tr>
</table>


